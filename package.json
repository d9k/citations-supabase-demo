{
  "scripts": {
    "example": "env-cmd -x echo \\$EXAMPLE_FILE_PATH",
    "example2": "env-cmd --use-shell \"echo test > \\$EXAMPLE_FILE_PATH\"",
    "storybook": "storybook dev --no-open -p 6006",
    "storybook:build": "storybook build",
    "sb": "pnpm run storybook",
    "db:dump": "pnpm run db:dump:data && pnpm run db:dump:schema && pnpm run db:dump:migrations",
    "db:dump:schema": "env-cmd --use-shell \"supabase db dump > \\$SUPABASE_DUMP_SCHEMA_PATH\"",
    "db:dump:data": "pnpm run db:dump:data:with-auth-logs && pnpm run db:dump:data:cleaning",
    "db:dump:data:with-auth-logs": "env-cmd --use-shell \"supabase db dump --data-only > \\$SUPABASE_DUMP_DATA_PATH\"",
    "db:dump:data:cleaning": "tsx script-node/supabase-dump-cleaning.ts",
    "db:dump:migrations": "env-cmd --use-shell \"supabase db dump --data-only --schema supabase_migrations > \\$SUPABASE_DUMP_MIGRATIONS_PATH\"",
    "db:gen:descriptions": "tsx script-node/db-gen-descriptions.ts",
    "db:gen:json-schema": "pnpm run db:gen:typescript && pnpm run db:gen:parse-types-to-json-schema && pnpm run db:gen:descriptions && pnpm run db:gen:json-schema-add-descriptions",
    "db:gen:parse-types-to-json-schema": "tsx script-node/db-parse-types-to-json-schema.ts",
    "db:gen:json-schema-add-descriptions": "tsx script-node/db-json-schema-add-descriptions.ts",
    "db:gen:typescript": "env-cmd --use-shell \"supabase gen types typescript --linked > \\$SUPABASE_GENERATED_TYPES_PATH\"",
    "db:migrations": "env-cmd -x supabase migration list",
    "db:migrations:create:from-remote": "pnpm run db:migrations && env-cmd -x supabase db pull --schema auth,public,extensions && pnpm run db:migrations"
  },
